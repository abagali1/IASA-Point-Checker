{
  "rules": {
    ".write": false,
    // ".read": true,
    "choreos": {
      "$uniqname": {
        ".read": "auth.token.email.contains($uniqname)",
      }
    },
    "dancers": {
      "$dance": {
        ".read": "root.child('choreos').child(auth.token.email.replace('@umich.edu', '')).val() == $dance",
      }
    },
    "members": {
      "$uniqname":{
        ".read": "auth.token.email.contains($uniqname)",
      }
    }
  }
}
